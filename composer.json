{
	"name": "automattic/jetpack-gif-block",
	"description": "Show a gif",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"php-mock/php-mock": "^2.1"
	},
	"autoload": {
		"files": [
			"actions.php"
		],
		"classmap": [
			"src/"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": "../*"
		}
	],
	"scripts": {
		"phpunit": [
			"@composer install",
			"./vendor/phpunit/phpunit/phpunit --colors=always"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
